@using MilkTea_CNWeb.Models
@model List<SanPham>




    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />



    <script src="~/Scripts/bootstrap.min.js"></script>




<table class="table">
    <thead>
        <tr>

            <th scope="col">Tên </th>
            <th scope="col">Giá</th>

        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>

                    <td>@item.TenSanPham</td>
                    <td>@item.Gia</td>
                </tr>
            }
        }


    </tbody>
</table>
